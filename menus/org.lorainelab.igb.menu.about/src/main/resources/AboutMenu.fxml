<StackPane prefHeight="308.0" prefWidth="661.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <StackPane prefHeight="64.0" prefWidth="550.0">
               <children>
                  <Label fx:id="titleLabel" prefHeight="39.0" prefWidth="399.0" textAlignment="CENTER">
                     <font>
                        <Font size="16.0" />
                     </font>
                  </Label>
               </children>
            </StackPane>
            <VBox fx:id="vBox" minHeight="73.0" minWidth="500.0" prefHeight="96.0" prefWidth="503.0">
               <VBox.margin>
                  <Insets bottom="10.0" top="10.0" />
               </VBox.margin>
               <children>
                  <StackPane fx:id="mainTextContainer" prefHeight="150.0" prefWidth="554.0" />
               </children>
            </VBox>
            <StackPane prefHeight="97.0" prefWidth="550.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="108.0" fitWidth="111.0" pickOnBounds="true" preserveRatio="true">
                     <StackPane.margin>
                        <Insets bottom="10.0" top="10.0" />
                     </StackPane.margin>
                  </ImageView>
               </children>
            </StackPane>
            <StackPane>
               <children>
                  <Button fx:id="okBtn" mnemonicParsing="false" prefHeight="26.0" prefWidth="78.0" text="OK">
                     <StackPane.margin>
                        <Insets bottom="10.0" />
